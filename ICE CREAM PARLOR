#include<stdio.h>
int main(){
    int t ;
    scanf("%d",&t);
    while ( t--){
        int money,n,i;
        scanf("%d",&money);
        scanf("%d",&n);
        int arr[n];
        for(i=0;i<n;i++){
            scanf("%d",&arr[i]);
        }
        int j ;
        for(i=0;i<n-1;i++){
            for(j=i+1;j<n;j++){
                if(arr[i]+arr[j]==money){
                    printf("%d %d\n",i+1,j+1);
                }
            }
        }   
    }
    return 0 ;
}
